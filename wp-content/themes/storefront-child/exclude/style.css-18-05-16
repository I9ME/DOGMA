.main-navigation ul li:nth-child(8),
.main-navigation ul li:nth-child(9),
.main-navigation ul li:nth-child(10){
  width:16.6666%;
  width:calc(16.6666667% - 0px);
}

.main-navigation ul li a{
  font-size:12px;
  height:100%;
}
.main-navigation ul li a:hover{
  background:#EEE;
}

.main-navigation ul li:nth-child(1) a:before,
.main-navigation ul li:nth-child(2) a:before,
.main-navigation ul li:nth-child(3) a:before,
.main-navigation ul li:nth-child(4) a:before{
  position:relative;
  display:inline-block;
  width:15px;
  height:14px;
  content:"";
  top:2px;
}

.main-navigation ul li:nth-child(1) a:before{
  background:url('images/img_all.png') -280px 0 no-repeat;
}

.main-navigation ul li:nth-child(2) a:before{
  background:url('images/img_all.png') -240px 0 no-repeat;
  width:18px;
}

.main-navigation ul li:nth-child(3) a:before{
  content: "\f0e5" !important;
  font-family:"fontAwesome";
  font-size:14px;
  top:0;
}

.main-navigation ul li:nth-child(4) a:before{
  background:url('images/img_all.png') -260px 0 no-repeat;
}




  
}




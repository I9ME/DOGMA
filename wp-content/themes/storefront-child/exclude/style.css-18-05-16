/*DiogoEdit*/



.loginCadastro{
  position:relative;
  display:block;
  float:right;
  margin-left:5% !important;
}

.loginCadastro .icon{
  position:relative;
  display:block;
  float:right;
  width:32px;
  height:32px;
  margin:0px 10px 0 0px;
  background:url('images/img_all.png') no-repeat;
}

.loginCadastro:hover .icon{
  background:url('images/img_all.png') 0 -32px;
}

.loginCadastro .link{
  position:relative;
  display:table;
  float:right;
  width:auto;
  height:32px;
  font-size:11px;
  color:#000;
  height:32px;
  line-height:40px;
}
.loginCadastro .link:hover{
  color:#888;
}

.loginCadastro .link .text{
  position: relative;
  display: block;
  float: right;
  height: 32px;
  line-height: 32px;
  padding-right:15px;
}
.loginCadastro .link .text:before{
  position:absolute;
  display:block;
  right:0;
  top:-3px;
  content: "\f0dd";
  font-family: 'FontAwesome';
}


.loginCadastro .dropDown{
  position:absolute;
  display:none;
  min-width:280px;
  padding:20px;
  background:#FFF;
  border:1px solid #DDD;
  z-index:21;
  top:30px;
  right:0;
}
.loginCadastro .dropDown:before{
  position:absolute;
  display:block;
  top:-2px;
  width:10px;
  height:5px;
  line-height:5px;
  content: "\f0de";
  font-family: 'FontAwesome';
  right:20px;
  color:#DDD;
}
.loginCadastro .dropDown.active{
  display:block;
}


.loginCadastro .dropDown.active .userLogged{
  position:relative;
  display:block;
}
.loginCadastro .dropDown.active .userLogged .listLinks{
  position:relative;
  display:block;
  margin:0;
  padding:0;
  list-style:none;
}

.loginCadastro .dropDown.active .userLogged .listLinks li{
  position:relative;
  display:block;
  margin:0;
  padding:0;
}

.loginCadastro .dropDown.active .userLogged .listLinks li a{
  position:relative;
  display:block;
  width:100%;
  margin:0;
  padding:0;
}


